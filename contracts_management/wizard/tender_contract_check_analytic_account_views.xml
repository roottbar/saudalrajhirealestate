<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_tender_contract_check_analytic_account_form" model="ir.ui.view">
            <field name="name">view.tender.contract.check.analytic.account.form</field>
            <field name="model">tender.contract.check.analytic.account.wizard</field>
            <field name="arch" type="xml">
                <form string="Confirm">
                    <span class="o_form_label">Assign analytic account and tags from contract.</span>
                    <br/>
                    <br/>
                    <field name="assign_analytic" widget="radio"/>
                    <footer>
                        <button string="Cancel" class="btn-secondary" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>
    </data>
</odoo>

