<?xml version="1.0" ?>
<odoo>
    <data noupdate="1">
        <record id="sequence_tender_project" model="ir.sequence">
            <field name="name">Project Sequence</field>
            <field name="code">tender.project</field>
            <field name="prefix">P/</field>
            <field name="padding">4</field>
            <field eval="False" name="company_id"/>
        </record>

        <record id="sequence_tender_contract" model="ir.sequence">
            <field name="name">Contract Sequence</field>
            <field name="code">tender.contract</field>
            <field name="prefix">CON/</field>
            <field name="padding">4</field>
            <field eval="False" name="company_id"/>
        </record>

        <record id="sequence_tender_quotation" model="ir.sequence">
            <field name="name">Quotation Sequence</field>
            <field name="code">tender.quotation</field>
            <field name="prefix">Q/</field>
            <field name="padding">4</field>
            <field eval="False" name="company_id"/>
        </record>

        <record id="sequence_tender_customer" model="ir.sequence">
            <field name="name">Customer Sequence</field>
            <field name="code">tender.customer</field>
            <field name="prefix">CUS/</field>
            <field name="padding">4</field>
            <field eval="False" name="company_id"/>
        </record>
    </data>
</odoo>