<odoo>
    <data>
        <record id="renting_custom_view_move_form" model="ir.ui.view">
            <field name="name">account.move</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_move_form"/>
            <field name="arch" type="xml">
                <xpath expr="//div[@name='button_box']" position="inside">
                    <button type="object" name="action_view_renting_order" class="oe_stat_button"
                            icon="fa-pencil-square-o" string="Renting Order" groups="rent_customize.show_renting_order_group">
                    </button>
                </xpath>
            </field>
        </record>
    </data>
</odoo>