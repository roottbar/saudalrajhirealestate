<?xml version="1.0" encoding="utf-8"?>

    <odoo>
        <record id="sequence" model="ir.sequence">
                <field name="name">Contract sequence</field>
                <field name="code">contract.sequence</field>
                <field name="prefix">C</field>
                <field name="padding">5</field>
                <field name="company_id" eval="False"/>
            </record>

            <record id="sequence_two" model="ir.sequence">
                <field name="name">Power Of Attorney sequence</field>
                <field name="code">power.of.attorney.sequence</field>
                <field name="prefix">P</field>
                <field name="padding">5</field>
                <field name="company_id" eval="False"/>
            </record>


</odoo>