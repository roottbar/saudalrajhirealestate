<?xml version="1.0" encoding="utf-8"?>

<odoo>
    <data>
        <record model="ir.ui.view" id="rent_property_elevator_tree">
            <field name="name">rent.property.elevator.tree</field>
            <field name="model">rent.property.elevator</field>
            <field name="arch" type="xml">
                <tree string="Elevator">
                    <field name="elevator_maintenance_description"/>
                    <field name="elevator_number_name"/>
                    <field name="elevator_maintenance_date"/>
                    <field name="elevator_maintenance_value"/>
                </tree>
            </field>
        </record>
        <record model="ir.ui.view" id="rent_property_elevator_form">
            <field name="name">rent.property.elevator.form</field>
            <field name="model">rent.property.elevator</field>
            <field name="arch" type="xml">
                <form string="Elevator Maintenance">
                    <sheet>
                        <group>
                            <group>
                                <field name="elevator_number_name"/>
                                <field name="elevator_maintenance_description"/>
                            </group>
                            <group>
                                <field name="elevator_maintenance_date"/>
                                <field name="elevator_maintenance_value"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
    </data>
</odoo>