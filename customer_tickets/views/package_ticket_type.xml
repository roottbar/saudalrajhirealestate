<odoo>
    <data>
        <record id="ticket_type_view_form" model="ir.ui.view">
            <field name="name">ticket_type_view_form</field>
            <field name="model">sub.ticket.type</field>
            <field name="arch" type="xml">
                <form string="ticket_type_form">
                    <sheet>
                        <group >
                            <field name="name" readonly="1"/>
                            <field name="numbers" readonly="1"/>
                            <field name="consumed" readonly="1"/>
                            <field name="available" readonly="1" widget="boolean_toggle"/>
                            <field name="active" readonly="1"/>
                            <field name="plus_id" readonly="1"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
