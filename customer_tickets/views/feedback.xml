<odoo>
    <data>
        <record id="feedback_view_form" model="ir.ui.view">
            <field name="name">feedback_view_form</field>
            <field name="model">feedback</field>
            <field name="arch" type="xml">
                <form string="feedback_form">
                    <sheet>
                        <group>
                            <field name="name"/>
                            <field name="customer_ticket_id" invisible="1"/>
                        </group>
                    </sheet>
                    <footer>
                        <button name="send2plus" class="oe_highlight" type="object" string="Send Feedback"/>
                    </footer>
                </form>
            </field>
        </record>
    </data>
</odoo>