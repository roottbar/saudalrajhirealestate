<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <!-- Default Job Position Categories -->
        <record id="job_category_technical" model="hr.job.category">
            <field name="name">Technical</field>
        </record>
        
        <record id="job_category_management" model="hr.job.category">
            <field name="name">Management</field>
        </record>
        
        <record id="job_category_sales" model="hr.job.category">
            <field name="name">Sales &amp; Marketing</field>
        </record>
        
        <!-- Default Resume Analysis Configuration -->
        <record id="default_analysis_config" model="ir.config_parameter">
            <field name="key">hr_resume_ats.default_analysis_enabled</field>
            <field name="value">True</field>
        </record>
        
    </data>
</odoo>