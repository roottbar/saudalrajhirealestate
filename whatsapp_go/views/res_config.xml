<odoo>
  <data>
    <record model="ir.ui.view" id="setting_inherit_form_view">
      <field name="name">res.config.settings.inherit</field>
      <field name="model">res.config.settings</field>
      <field name="inherit_id" ref="base.res_config_settings_view_form"/>
      <field name="arch" type="xml">
        <xpath expr="//div[@id='invite_users']" position="inside">
          <div id="go4_whatsapp">
            <h2>Go4whatsapp</h2>
            <block title="Go4Whatsapp" >
              <setting id="go4whatsapp_url" help="Go4whatsapp url">
                <div class="row mt16 o_settings_container">
                  <div class="col-xs-12 col-md-6 o_setting_box">
                    <label for="go4whatsapp_url" string="Go4Whatsapp URL"/>
                    <field name="go4whatsapp_url"  class="o_inline"/>
                  </div>
                </div>
                <div class="row mt16 o_settings_container">
                  <div class="col-xs-12 col-md-6 o_setting_box">
                    <label for="template_id" />
                    <field name="template_id"  class="o_inline"/>
                  </div>
                </div>
                <div class="row mt16 o_settings_container">
                  <div class="col-xs-12 col-md-6 o_setting_box">
                    <label for="org_id" />
                    <field name="org_id"  class="o_inline"/>
                  </div>
                </div>
               <div class="row mt16 o_settings_container">
                  <div class="col-xs-12 col-md-6 o_setting_box">
                    <label for="Subscription_expire_date" />
                    <field name="Subscription_expire_date"  class="o_inline"/>
                  </div>
                </div>
              </setting>
            </block>
          </div>
        </xpath>
      </field>
    </record>
  </data>
</odoo>
