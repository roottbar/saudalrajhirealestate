        <record id="action_report_budget_xlsx" model="ir.actions.report">
            <field name="name">تقرير الميزانية (XLSX)</field>
            <field name="model">budget.budget</field>
            <field name="report_type">qweb-html</field>
            <field name="report_name">budget_management.report_budget_template</field>
        </record>
    </data>
</odoo>