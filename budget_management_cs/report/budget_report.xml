<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="action_report_budget" model="ir.actions.report">
            <field name="name">تقرير الميزانية</field>
            <field name="model">budget.budget</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">budget_management.report_budget_template</field>
            <field name="print_report_name">(object.name or 'Budget')</field>
