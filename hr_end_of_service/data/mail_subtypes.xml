<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="mt_end_of_service_draft" model="mail.message.subtype">
            <field name="name">Draft</field>
            <field name="res_model">hr.end.service</field>
            <field name="default" eval="True"/>
            <field name="description">Reset End Of Service to draft</field>
        </record>
        <record id="mt_end_of_service_in_progress" model="mail.message.subtype">
            <field name="name">In Progress</field>
            <field name="res_model">hr.end.service</field>
            <field name="default" eval="True"/>
            <field name="description">End Of Service in progress</field>
        </record>
        <record id="mt_end_of_service_approved" model="mail.message.subtype">
            <field name="name">Approved</field>
            <field name="res_model">hr.end.service</field>
            <field name="default" eval="True"/>
            <field name="description">End Of Service has been rejected</field>
        </record>
        <record id="mt_end_of_service_rejected" model="mail.message.subtype">
            <field name="name">Rejected</field>
            <field name="res_model">hr.end.service</field>
            <field name="default" eval="True"/>
            <field name="description">End Of Service has been rejected</field>
        </record>
    </data>
</odoo>