<?xml version="1.0" ?>
<odoo>
    <data noupdate="1">
        <record id="sequence_petty_cash" model="ir.sequence">
            <field name="name">Petty Cash Sequence</field>
            <field name="code">petty.cash</field>
            <field name="prefix">PC/</field>
            <field name="padding">4</field>
            <field eval="False" name="company_id"/>
        </record>

        <record id="sequence_petty_cash_user_rule" model="ir.sequence">
            <field name="name">Petty.Cash.User Rule Sequence</field>
            <field name="code">petty.cash.user.rule</field>
            <field name="prefix">PettyRule/</field>
            <field name="padding">4</field>
            <field eval="False" name="company_id"/>
        </record>

        <record id="sequence_petty_cash_request_feeding" model="ir.sequence">
            <field name="name">Petty Cash Request Feeding Sequence</field>
            <field name="code">petty.cash.request.feeding</field>
            <field name="prefix">RF/</field>
            <field name="padding">4</field>
            <field eval="False" name="company_id"/>
        </record>

        <record id="sequence_petty_cash_request" model="ir.sequence">
            <field name="name">Petty Cash Request Sequence</field>
            <field name="code">petty.cash.request</field>
            <field name="prefix">PCR/</field>
            <field name="padding">4</field>
            <field eval="False" name="company_id"/>
        </record>

        <record id="sequence_petty_payment_voucher" model="ir.sequence">
            <field name="name">Petty Payment Voucher</field>
            <field name="code">petty.payment.voucher</field>
            <field name="prefix">PPV/</field>
            <field name="padding">4</field>
            <field eval="False" name="company_id"/>
        </record>
    </data>
</odoo>