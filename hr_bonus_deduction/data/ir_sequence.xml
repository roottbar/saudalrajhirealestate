<?xml version="1.0" ?>
<odoo>
    <data noupdate="1">
        <record id="sequence_hr_bonus" model="ir.sequence">
            <field name="name">Hr Bonus</field>
            <field name="code">hr.bonus</field>
            <field name="prefix">Bon/%(year)s/%(month)s/</field>
            <field name="padding">3</field>
        </record>

        <record id="sequence_hr_deduction" model="ir.sequence">
            <field name="name">Hr Deduction</field>
            <field name="code">hr.deduction</field>
            <field name="prefix">Ded/%(year)s/%(month)s/</field>
            <field name="padding">3</field>
        </record>
    </data>
</odoo>
